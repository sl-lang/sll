; Sll Quadratic Equation Solver Example
; @krzem5, 2021-10-31
(# sqrt)
(-- "math.sll")



(= solve (,,, a b c {
	(= d (- (* b b) (* 4 a c)))
	(?
		(< d 0) (@@ [])
		(= d 0) (@@ [(/ b (* -2 a))])
		{
			(= sqrt_d (<- sqrt d))
			(@@ [
				(/ (- b sqrt_d) (* -2 a))
				(/ (+ b sqrt_d) (* -2 a))
			])
		}
	)
}))



(= a 1)
(= b -1)
(= c -6)
(:> a "*x*x + " b "*c + " c " = 0 => x âˆˆ " (<- solve a b c) "\n")
