(= print_array (,,, a
	(:> "[")
	(-> (= idx 0) (< idx ($ a))
		(? idx (:> ", "))
		(:> (: a idx))
		(= idx (+ idx 1))
	)
	(:> "]")
))
(:> "Array: ") (<- print_array arr) (:> "\n" "")
(= map <1 "a" 2 "b" 3>)
