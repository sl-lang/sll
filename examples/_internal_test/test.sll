(= print_map (,,, m
	(:> "<")
	(= k (:: m map_key_type))
	(-> (= idx 0) (< idx ($ m))
		(? idx (:> ", "))
		(:> (: k idx) ": " (: m (: k idx)))
		(= idx (+ idx 1))
	)
	(:> ">")
))
