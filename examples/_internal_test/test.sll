(-> (= i 0) (<= i 20) {
	(? (&& (! (% i 2)) (! (% i 3))) {
		(:> "Multiple of 6: " i "\n")
	})
	(? (|| (! (% i 3)) (! (% i 5))) {
		(:> "Multiple of 3 or 5: " i "\n")
	})
	(++ i)
})

(:> "Checksum of 'abc': " (<- string$checksum "abc") "\n")

