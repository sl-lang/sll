(# int_type func_type string_type)
(-- "types.sll")

(= platform (<- (... "sys_get_platform")))

(= argc (<- (... "sys_arg_get_count")))
(= argv (... "sys_arg_get"))

(= executable (<- (... "sys_get_executable")))

(= sys (. (&:
	(~ string_type) platform
	(~ int_type) argc
	(~ func_type) argv
	(~ string_type) executable
) platform argc argv executable))
(## sys)
