(--- "types.sll")



(= time_module_type (&:
	(~ func_type) time
	(~ func_type) time_ns
	(~ func_type) sleep
	(~ func_type) sleep_ns
))



(= get (... "sll:time_current"))

(= get_ns (... "sll:time_current_ns"))

(= sleep (... "sll:time_sleep"))

(= sleep_ns (... "sll:time_sleep_ns"))


(= time (. time_module_type
	get
	get_ns
	sleep
	sleep_ns
))
(## time)
