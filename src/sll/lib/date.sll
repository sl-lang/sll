(# from_time)



(= time_zone_type (&:
	(,,, dt {
		(= h (// dt$offset 60))
		(= min (- dt$offset (* h 60)))
		(= off_str "")
		(?
			h {
				(? min {
					(= off_str (<- string$format " offset=%uh %umin" h min))
				} {
					(= off_str (<- string$format " offset=%uh" h))
				})
			}
			min {
				(= off_str (<- string$format " offset=%umin" h min))
			}
		)
		(@@ (<- string$format "<name=%s%s>" dt$name off_str))
	}) @@string@@
	int_type offset
	string_type name
))

(= date_type (&:
	(,,, dt {
		(= h (// dt$time_zone$offset 60))
		(= min (- dt$time_zone$offset (* h 60)))
		(@@ (<- string$format "%.4u/%.2u/%.2u %.2u:%.2u:%.2u %+.2d:%.2u (%s)"
			dt$year
			(+ dt$month 1)
			(+ dt$day 1)
			dt$hour
			dt$minute
			(:: dt$second int_type)
			h
			min
			dt$time_zone$name
		))
	}) @@string@@
	int_type year
	int_type month
	int_type day
	int_type week_day
	int_type hour
	int_type minute
	float_type second
	time_zone_type time_zone
))

(= date_module_type (&:
	(~ type_type) time_zone_type
	(~ type_type) date_type
	(~ array_type) DAYS
	(~ time_zone_type) LOCAL_TIME_ZONE
	(~ array_type) MONTHS
	(~ map_type) TIME_ZONE_NAMES
	(~ time_zone_type) UTC_TIME_ZONE
	(~ func_type) current
	(~ func_type) from_time
	(~ func_type) from_time_ns
	(~ func_type) to_time
))



(= LOCAL_TIME_ZONE (:: (<- (... "sll:date_get_time_zone")) time_zone_type))

(= UTC_TIME_ZONE (. time_zone_type 0 "UTC"))

