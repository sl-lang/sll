(= current (,,,
	(@@ (<- from_time (<- time$time) LOCAL_TIME_ZONE)))
)

(= from_time (,,, tm tz
	(= tz (?: (=== tz nil) LOCAL_TIME_ZONE (:: tz time_zone_type)))
	(= dt (<- (... "sll:date_split") (+ (:: tm float_type) (* tz$offset 60))))
	(@@ (. date_type
		(: dt 0)
		(: dt 1)
		(: dt 2)
		(: dt 3)
		(: dt 4)
		(: dt 5)
		(: dt 6)
		tz
	))
))

(= from_time_ns (,,, tm tz
	(@@ (<- from_time
		(* (:: tm int_type) 1e-9)
		tz
	))
))

(= to_time (,,, dt
	(= dt (:: dt date_type))
	(@@ (- (<- (... "sll:date_merge")
		dt$year
		dt$month
		dt$day
		dt$hour
		dt$minute
		dt$second
	) (* dt$time_zone$offset 60)))
))



(= date (. date_module_type
	time_zone_type
	date_type
	DAYS
	LOCAL_TIME_ZONE
	MONTHS
	TIME_ZONE_NAMES
	UTC_TIME_ZONE
	current
	from_time
	from_time_ns
	to_time
))
(## date)
