(--- "types.sll" "map.sll")



(= ERROR_INTERNAL_ERROR (&:
	(,,, dt
		(@@ (+
			dt$name
			(?: dt$raw_error
				(+
					" ["
					dt$raw_error
					']'
				)
				""
			)
			": "
			dt$str
		))
	) @@string@@
	(~ string_type) name
	(~ string_type) str
	int_type raw_error
))

(= error_codes_type (&:
	(~ type_type) ERROR_INTERNAL_ERROR
	(~ map_type) SYSTEM_ERROR_CODES
	(~ func_type) get_by_name
))



