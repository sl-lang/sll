(= dt (:: SYSTEM_ERROR_CODES map_value_type))
(= _name_to_error ({> (= i 0) (< i ($ dt))
	(= e (: dt i))
	(++ i)
	e$name e
))
(<- map$remove _name_to_error "EWIN")



(= get_by_name (,,, name
	(@@ (: _name_to_error (:: name string_type)))
))



(= error_codes (. error_codes_type
	ERROR_INTERNAL_ERROR
	SYSTEM_ERROR_CODES
	get_by_name
))
(## error_codes)
