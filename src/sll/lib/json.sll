(# func_type handle_type)
(-- "types.sll")

(= json_null (<- (... "json_type") 0))
(= json_true (<- (... "json_type") 1))
(= json_false (<- (... "json_type") 2))

(= parse (... "json_parse"))
(= stringify (... "json_stringify"))

(= json (. (&:
	(~ handle_type) null
	(~ handle_type) true
	(~ handle_type) false
	(~ func_type) parse
	(~ func_type) stringify
) json_null json_true json_false parse stringify))
(## json)
