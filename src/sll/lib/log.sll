(# func_type)
(--- "types.sll")

(= log_f (... "sll:log_log"))
(= set_default (... "sll:log_set_default"))
(= set_file (... "sll:log_set_file"))
(= set_function (... "sll:log_set_function"))

(= log (. (&:
	(~ func_type) log
	(~ func_type) set_default
	(~ func_type) set_file
	(~ func_type) set_function
) log_f set_default set_file set_function))
(## log)
