(# func_type)
(--- "types.sll")

(= stdin -1)
(= stdout -2)
(= stderr -3)

(= close (... "sll:file_close"))
(= open (... "sll:file_open"))
(= read (... "sll:file_read"))
(= write (... "sll:file_write"))

(= file (. (&:
	(~ func_type) close
	(~ func_type) open
	(~ func_type) read
	(~ func_type) write
) close open read write))
(## stdin stdout stderr file)
