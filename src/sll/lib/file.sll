(# func_type)
(-- "types.sll")

(= stdin -1)
(= stdout -2)
(= stderr -3)

(= close (... "file_close"))
(= open (... "file_open"))
(= read (... "file_read"))
(= write (... "file_write"))

(= file (. (&:
	(~ func_type) close
	(~ func_type) open
	(~ func_type) read
	(~ func_type) write
) close open read write))
(## stdin stdout stderr file)
