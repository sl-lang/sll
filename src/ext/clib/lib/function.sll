(---
	"clib/_load_library.sll"
	"clib/library.sll"
	"types.sll"
)



(= clib_function_type (&:
	(~ string_type) name
	(~ clib_library$clib_library_type) library
))

(= clib_function_module_type (&:
	(~ type_type) clib_function_type
))



(= clib_function (. clib_function_module_type
	clib_function_type
))
(## clib_function)
