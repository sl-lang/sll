(---
	"gfx/_load_library.sll"
	"weakref.sll"
	"window_manager/util.sll"
)



(= window_manager:util$__gfx_callbacks$delete (,,, window
	(= window$_gfx_context weakref$NIL_REF)
))

(= window_manager:util$__gfx_callbacks$resize (,,, window
	(= ctx (<- weakref$get window$_gfx_context))
	(? (!== ctx weakref$WEAKREF_NO_OBJECT) (<- (... "gfx:context_resize") ctx$_handle))
))
